{
  "name": "career-boost",
  "version": "1.0.0",
  "description": "AI-powered interview preparation platform",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd backend && npm run dev",
    "dev:frontend": "cd frontend/career-boost && npm run dev",
    "build": "npm run build:backend && npm run build:frontend",
    "build:backend": "cd backend && npm run build",
    "build:frontend": "cd frontend/career-boost && npm run build",
    "start": "cd backend && npm start",
    "install:all": "npm install && cd backend && npm install && cd ../frontend/career-boost && npm install",
    "vercel-build": "cd frontend/career-boost && npm run build"
  },
  "keywords": [
    "interview",
    "preparation",
    "AI",
    "career",
    "development"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}